<h1>Wishlists</h1>


<div class="container">
  <div class="index-img">
      <% @wishlists.each do |w| %>

          <div class="img-container">

            <%= link_to image_tag(w.image, :class => 'img-thumbnail'), product_path %>
            <div class="text_description">
            <div class="text">
            <%= link_to w.title, product_path%>
            </div>
            </div>
          </div>
      <% end %>
  </div>
</div>
